<UserControl x:Class="wp7_api_demos.View.Controls.info.PushNotificationInfoMessage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="480" d:DesignWidth="480">
    
    <Grid x:Name="LayoutRoot">
<StackPanel>
            <TextBlock Text="{Binding Path=LocalizedResources.dialogPushNotificationsText1, Source={StaticResource LocalizedStrings}}" TextWrapping="Wrap" Margin="10"/>

            <TextBlock Text="{Binding Path=LocalizedResources.dialogPushNotificationsText2, Source={StaticResource LocalizedStrings}}" TextWrapping="Wrap" Margin="10"/>

        </StackPanel>
    </Grid>
</UserControl>
